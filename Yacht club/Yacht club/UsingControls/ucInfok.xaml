<UserControl x:Class="Yacht_club.UsingControls.ucInfok"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Yacht_club.UsingControls"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="585">
    <Grid Background="#FF1D212C">
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="325"/>
        </Grid.RowDefinitions>
        <Label Style="{DynamicResource MenuDecLabel}" Content="Információk:" HorizontalAlignment="Left" VerticalAlignment="Center" FontWeight="Bold" Grid.Column="0" Grid.Row="0"/>
        <StackPanel HorizontalAlignment="Left" Height="325" VerticalAlignment="Top" Width="329" Grid.Row="1">
            <Label Style="{DynamicResource MenuDecLabel}" Content="Elerhetőség: "/>
            <Label Style="{DynamicResource MenuDecLabel}" Content="Készítők: "/>
            <Label Style="{DynamicResource MenuDecLabel}" Content="Valamik_1: "/>
            <Label Style="{DynamicResource MenuDecLabel}" Content="Valamik_2: "/>
        </StackPanel>

    </Grid>
</UserControl>
