<UserControl x:Class="Yacht_club.UsingControls.ucErtesitesek"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Yacht_club.UsingControls"
             mc:Ignorable="d" 
             d:DesignHeight="125" d:DesignWidth="885">
    <Grid SnapsToDevicePixels="True" Background="#FF2D3446">
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="listBox" Height="100" Grid.Row="1" Width="885" Margin="10,0,10,10"/>
        <Menu x:Name="menu" Height="25" Grid.Row="0" Width="885" Margin="10,0"/>


    </Grid>
</UserControl>
