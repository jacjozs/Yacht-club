<UserControl x:Class="Yacht_club.UsingControls.ucErtesitesek"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Yacht_club.UsingControls"
             mc:Ignorable="d" 
             d:DesignHeight="115" d:DesignWidth="885">
    <Grid SnapsToDevicePixels="True" Background="#FF2D3446">
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="115"/>
        </Grid.RowDefinitions>
        <Label Content="Ablak Takarítása" FontSize="14" Cursor="Hand" Foreground="#FFACB5B9" Padding="0,2,0,0" BorderThickness="10,0,0,0" Grid.Row="0"/>
        <ListBox x:Name="listBox" Height="115" Grid.Row="1" Width="885" Margin="10,0,10,10"/>
    </Grid>
</UserControl>
