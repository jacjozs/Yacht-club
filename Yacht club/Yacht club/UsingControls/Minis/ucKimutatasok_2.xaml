<UserControl x:Class="Yacht_club.UsingControls.Minis.ucKimutatasok_2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Yacht_club.UsingControls.Minis"
             mc:Ignorable="d" 
             d:DesignHeight="202" d:DesignWidth="416.5" Background="{Binding uc_hatter}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition Height="182"/>
        </Grid.RowDefinitions>
        <Label Style="{DynamicResource MenuDecLabel}" Content="Heti bérlések:" Foreground="{Binding betu_szin}" HorizontalAlignment="Left" Padding="15 2 0 0" VerticalAlignment="Center" FontWeight="Bold" Grid.Column="0" Grid.Row="0"/>
        <StackPanel x:Name="stDiagram" Orientation="Horizontal" Height="157" Width="416.5" Grid.Row="1" VerticalAlignment="Top"/>
        <StackPanel x:Name="stNames" Orientation="Horizontal" Width="416.5" Height="25" Grid.Row="1" VerticalAlignment="Bottom"/>
    </Grid>
</UserControl>
