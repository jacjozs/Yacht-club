<UserControl x:Class="Yacht_club.UsingControls.ucBeallitasok"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Yacht_club.UsingControls"
             mc:Ignorable="d" 
             d:DesignHeight="540" d:DesignWidth="885">
    <Grid Background="#FF1D212C">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="lbMenuk" Content="Menü Sávok" Style="{DynamicResource SettingLabel}" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center"/>
        <CheckBox x:Name="checkBox" Content="Oldalsó Menü Sáv" HorizontalAlignment="Left" Margin="27,75,0,0" Grid.Column="0" Grid.Row="0" FontWeight="Bold" Foreground="#FFACB5B9"/>
        <CheckBox x:Name="checkBox1" Content="Felső Menü Sáv" HorizontalAlignment="Left" Margin="27,126,0,0" Grid.Column="0" Grid.Row="0" FontWeight="Bold" Foreground="#FFACB5B9" IsChecked="True"/>

    </Grid>
</UserControl>
